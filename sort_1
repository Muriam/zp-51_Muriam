#include <QCoreApplication>
#include <iostream>
#include <iomanip>
#include <time.h>
#include <cstdlib>
#include <ctime>
#include <locale>
#include <QtDebug>
#define SIZE 10


int main()
{

    setlocale(LC_ALL, "rus");
    srand(time(NULL)); // Инициализируем генератор случайных чисел

    int A[SIZE][SIZE];
    int i, j;



        qDebug() << "матрица A \n\n";
        for (i = 0; i<SIZE; i++)
        {
                for (j = 0; j<SIZE; j++)
                {
                        A[i][j] = rand() % 100;    // Каждый элемент случайному числу от 0 до 9
                        qDebug() << /*setw(4) <<*/ A[i][j];
                }
                qDebug() << endl;
        }
            qDebug() << endl;


    return 0;
}
